<script>
  function logout() {
    swal(
      {
        title: "Are you sure?",
        text: "Once logged out, you will need to log in again!",
        showCancelButton: true,
        confirmButtonText: "Logout",
        cancelButtonText: "Cancel",
        closeOnConfirm: false,
        closeOnCancel: true,
      },
      function (isConfirm) {
        if (isConfirm) {
          window.location.href = "/admin/logout";
        }
      }
    );
  }
</script>

